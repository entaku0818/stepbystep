//
//  Release環境用の設定例
//  使用方法: このファイルをRelease.xcconfigにコピーして、実際の値を設定してください
//

#include "Common.xcconfig"

// AdMob設定（本番用のIDに置き換えてください）
ADMOB_APP_ID = ca-app-pub-XXXXXXXXXXXX~XXXXXXXXXX
ADMOB_REWARDED_AD_UNIT_ID = ca-app-pub-XXXXXXXXXXXX/XXXXXXXXXX

// Firebase Functions設定
FIREBASE_FUNCTIONS_URL = https://us-central1-YOUR-PROJECT-ID.cloudfunctions.net

// API設定
API_ENVIRONMENT = production

// RevenueCat設定
REVENUECAT_API_KEY = appl_XXXXXXXXXXXXXXXXXXXXXXXXXX

// ビルド設定
ENABLE_USER_SCRIPT_SANDBOXING = NO